# TAMS Frontend Environment Variables
# Copy this file to .env and update the values as needed

# API Configuration
# Note: In production, the app automatically uses /api/proxy (Vercel proxy)
# In development, it uses /api (Vite dev server proxy)
# This variable is no longer needed but kept for backward compatibility
VITE_API_URL=http://34.216.9.25:8000

# Application Configuration
VITE_APP_TITLE=TAMS Demo
VITE_APP_DESCRIPTION=Time Addressable Media Storage Demo

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SEARCH=true

# Development
VITE_DEBUG_MODE=false 